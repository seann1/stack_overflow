<div class="row">
<h1>Log In</h1>
</div>

<%= form_tag sessions_path do %>
  <div class="row">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="row">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  <div class="row">
  <div class="actions"><%= submit_tag "Log In", class: "button"%></div>
  </div>
<% end %>
